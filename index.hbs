{
	"posts":[
	{{#get "posts" include="tags,author" limit="all"}}
		{{#foreach posts}}
				{{> post}}
				{{#unless @last}},{{/unless}}
		{{/foreach}}
	{{/get}}
	],

	"tags":[
	{{#get "tags" include="count" limit="all"}}
		{{#foreach tags}}
				{{> tag}}
				{{#unless @last}},{{/unless}}
		{{/foreach}}
	{{/get}}
	],

	"users":[
	{{#get "users" include="count" limit="all"}}
		{{#foreach users}}
				{{> user}}
				{{#unless @last}},{{/unless}}
		{{/foreach}}
	{{/get}}
	],

	"meta":{{> meta}}
}
